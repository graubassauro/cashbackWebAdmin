{
  "name": "cashbackwebadmin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prettier-format": "yarn prettier --config .prettierrc --write src",
    "lint-fix": "eslint --fix src",
    "lint": "tsc --noEmit && eslint src/**/*.{ts,tsx} --max-warnings=0",
    "lint-check": "eslint src --max-warnings=0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run prettier-format",
      "npm run lint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run check-types",
      "pre-push": "npm run prettier-check && npm run lint-check && npm run check-types"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@phosphor-icons/react": "^2.0.10",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
